{
  "name": "react-view",
  "version": "0.0.0-development",
  "description": "",
  "keywords": [],
  "main": "dist/react-view.umd.js",
  "module": "dist/react-view.es5.js",
  "typings": "dist/types/react-view.d.ts",
  "files": [
    "dist"
  ],
  "author": "Vojtech Miksu <vojtech@uber.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "travis-deploy-once": "travis-deploy-once",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@babel/code-frame": "^7.5.5",
    "@babel/generator": "^7.6.4",
    "@babel/preset-react": "^7.6.3",
    "@babel/template": "^7.6.0",
    "@babel/traverse": "^7.6.3",
    "@babel/types": "^7.6.3",
    "@miksu/prettier": "^1.18.2",
    "baseui": "^9.13.0",
    "copy-to-clipboard": "^3.2.0",
    "just-clone": "^3.1.0",
    "lodash": "^4.17.15",
    "prism-react-renderer": "^1.0.2",
    "react-simple-code-editor": "^0.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@types/babel__code-frame": "^7.0.1",
    "@types/babel__core": "^7.1.3",
    "@types/babel__generator": "^7.6.0",
    "@types/babel__template": "^7.0.2",
    "@types/babel__traverse": "^7.0.7",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.0.8",
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "colors": "^1.3.2",
    "commitizen": "^4.0.0",
    "cross-env": "^6.0.0",
    "cz-conventional-changelog": "^3.0.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "lint-staged": "^9.0.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "replace-in-file": "^4.1.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.15.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.3",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.23.0",
    "semantic-release": "^15.13.28",
    "shelljs": "^0.8.3",
    "styletron-engine-atomic": "^1.4.3",
    "styletron-react": "^5.2.4",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.0.3"
  },
  "volta": {
    "node": "10.17.0",
    "yarn": "1.19.1"
  }
